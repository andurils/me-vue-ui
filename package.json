{
  "name": "me-ui",
  "version": "0.1.0",
  "description": "A Vue.js 2.0 UI Toolkit for Web",
  "scripts": {
    "bootstrap": "yarn || npm i",
    "build:file": "node build/bin/build-entry.js",
    "lint": "eslint --fix examples  packages ",
    "clean": "rimraf dist && rimraf packages/*/lib && rimraf test/**/coverage",
    "build:dist": "cross-env NODE_ENV=development webpack --config  build/webpack.config.js",
    "build:theme": "node build/bin/gen-cssfile && gulp build --gulpfile packages/theme-gondor/gulpfile.js && cp-cli packages/theme-gondor/lib lib/theme-gondor",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.js",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "normalize-wheel": "^1.0.1",
    "throttle-debounce": "^3.0.1"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.13.5",
    "@vue/component-compiler-utils": "^3.2.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "cp-cli": "^2.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.6.0",
    "eslint-webpack-plugin": "^2.5.2",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^4.1.0",
    "highlight.js": "^10.6.0",
    "html-webpack-plugin": "^5.2.0",
    "json-templater": "^1.2.0",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.0.2",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^3.0.0",
    "node-sass": "^5.0.0",
    "prettier": "2.2.1",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "transliteration": "^2.2.0",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^4.1.1",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.6",
    "vue-router": "^3.5.1",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.24.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andurils/me-vue-ui.git"
  },
  "author": "anduril",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andurils/me-vue-ui/issues"
  },
  "homepage": "https://andurils.github.io/me-vue-ui",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
